//declaring the possible states HackCharm can be in :D 
enum HackCharmState 
{
  IDLE,
  TAP_REACTION,
  SHAKE_REACTION,
  DIZZY,
  SLEEP,
  WAKE
};

HackCharmState currentState = SLEEP;

// timing setup (timing is scary D:)
unsigned long lastMotionTime = 0; // Time since any motion
unsigned long lastStateChangeTime = 0; // Time since HackCharm entered its current state/expression

// Constants to define sensitivity 
const float TAP_THRESHOLD = 0.7;
const float SHAKE_THRESHOLD = 4.5;
const int SHAKE_COUNT_THRESHOLD = 1; //How many strong movements = shake ig

//
const unsigned long SHAKE_WINDOW_MS = 1000;
const unsigned long STATE_TIMEOUT_MS = 3000;
const unsigned long SLEEP_TIMEOUT_MS = 15000;
const unsigned long WAKE_DURATION_MS = 2000;
const unsigned long MIN_STATE_DURATION_MS = 2000;

int shakeCount = 0; //count of how many strong movements recently
bool wasShaking = false;

//function to check if the current animation has ran for long enough
bool hasStateRunLongEnough(unsigned long now) {
  return (now - lastStateChangeTime) >= MIN_STATE_DURATION_MS;
}

//function to draw the states :0
void drawState (HackCharmState state) {
  u8g2.clearBuffer(); // clear the internal memory
  
  switch (state) {
    case IDLE:
      u8g2.drawXBMP(0, 10, 128, 128, IdleFace); 
      break;
    case TAP_REACTION:
      //draw happy face ^v^
      u8g2.drawXBMP(0, 10, 128, 128, HappyFace); 
      break;
    case SHAKE_REACTION:
      //draw weee face :OOO
      u8g2.setFont(u8g2_font_logisoso20_tr);
      u8g2.drawStr(20, 80, "WHEEEE!");  // Replace with face later
      break;
    case DIZZY:
      //draw dizzy face @_@'
      u8g2.drawXBMP(0, 10, 128, 128, DizzyFace); 
      break;
    case SLEEP:
      //draw eeping face z_Z
      u8g2.setFont(u8g2_font_logisoso20_tr);
      u8g2.drawStr(20, 80, "sleepy!");  // Replace with face later
      break;
    case WAKE:
      u8g2.setFont(u8g2_font_logisoso20_tr);
      u8g2.drawStr(20, 80, "woke up!");  // Replace with face later
      break;
  }
  
  u8g2.sendBuffer(); // Push everything to the screen
}

// function to calculate magnitude of acceleration aka how much it is moving :0
float getAccelMagnitude() {
  xl.readXYZTData(XValue, YValue, ZValue, Temperature);
  
  float x = XValue * 0.0098;
  float y = YValue * 0.0098;
  float z = ZValue * 0.0098;
  float magnitude = sqrt(x*x + y*y + z*z);

  return magnitude;
}

void detectMotionAndTransition() {
  //time rn
  unsigned long now = millis();

  static float lastMagnitude = 0;
  float mag = getAccelMagnitude();
  float delta = abs(mag - lastMagnitude);
  lastMagnitude = mag;
  
  //print delta to serial for debugging
  Serial.println(delta);

  //Sleep detection
  if (currentState != SLEEP && (now - lastMotionTime) > SLEEP_TIMEOUT_MS) {
    currentState = SLEEP;
    lastStateChangeTime = now;
    return;
  }

  //Wake from sleep
  if (currentState == SLEEP && delta > 0.5) {
    currentState = WAKE;
    lastMotionTime = now;
    lastStateChangeTime = now;
    return;
  }

  //Record motion
  if (delta > 0.3){
    lastMotionTime = now;
  }

  //Go from wake to idle
  if (currentState == WAKE && now - lastStateChangeTime > WAKE_DURATION_MS)
  {
    currentState = IDLE;
    lastStateChangeTime = now;
    return;
  }

  //Go from reactions back to idle
  if ((currentState == TAP_REACTION || currentState == SHAKE_REACTION || currentState == DIZZY) && (now - lastStateChangeTime > STATE_TIMEOUT_MS))
  {
    if (hasStateRunLongEnough(now)) {
      currentState = IDLE;
      lastStateChangeTime = now;
      return;
    }
  }


  //detect shaking!
  if (delta >= SHAKE_THRESHOLD) {
    if (!wasShaking) {
      shakeCount = 1;
      wasShaking = true;
    }
    else {
      shakeCount++;
    }

    //dizzy/shaking reaction
    if (currentState == IDLE)
    {
      if (hasStateRunLongEnough(now)) {
        currentState = SHAKE_REACTION;
        Serial.println("SHAKE reaction triggered");
        lastStateChangeTime = now;  
      }
    }
    else if (currentState == SHAKE_REACTION && shakeCount >= SHAKE_COUNT_THRESHOLD)
    {
      if (hasStateRunLongEnough(now)) {
        currentState = DIZZY;
        Serial.println("DIZZY triggered");
        lastStateChangeTime = now;
      }
    }
    return;

  }
  else {
    //reset everything if delta becomes to low
    wasShaking = false;
    shakeCount = 0;
  }

  //detect taps!!

  if (delta > TAP_THRESHOLD && delta < SHAKE_THRESHOLD) {
    //debounce (ask)
    if (now - lastStateChangeTime > 2000 && currentState == IDLE) {
        if (hasStateRunLongEnough(now)) {
          int tapVariant = random(3); // This is a placeholder till I implement multiple tapping expressions
          currentState = TAP_REACTION;
          Serial.println("TAP detected");
          lastStateChangeTime = now;
        }
    }
    return;
  }



}


void setup() {
  Serial.begin(115200);
  u8g2.begin();
  xl.begin(D7);                  
  xl.beginMeasure();    
  //randomSeed(analogRead(0));
}

void loop(void) { 
  
  drawState(currentState);       // Output
  detectMotionAndTransition();   // Input + state updates
  delay(100);                    // 10 FPS 
}




